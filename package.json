{
  "name": "smolbot",
  "version": "2.0.0",
  "description": "Custom mod/misc/util bot",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "start:dev": "node -r dotenv/config dist/index.js",
    "start:prod": "npx prisma migrate deploy && node dist/index.js",
    "build": "tsc --build",
    "lint": "eslint src/ && prettier --check src/ && tsc --noEmit",
    "lint:fix": "eslint src/ --fix && prettier --write src/ && tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AtoraSuunva/smolbot.git"
  },
  "author": "AtoraSuunva",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AtoraSuunva/smolbot/issues"
  },
  "homepage": "https://github.com/AtoraSuunva/smolbot#readme",
  "dependencies": {
    "@prisma/client": "^4.8.1",
    "@types/convert-units": "^2.3.5",
    "convert-units": "^2.3.4",
    "discord.js": "^14.7.1",
    "dotenv": "^16.0.3",
    "emoji-regex": "^10.2.1",
    "env-var": "^7.3.0",
    "pino": "^8.8.0",
    "pino-pretty": "^9.1.1",
    "pretty-ms": "^8.0.0",
    "sleetcord": "^0.7.1",
    "undici": "^5.14.0"
  },
  "devDependencies": {
    "@types/humanize-duration": "^3.27.1",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.26.0",
    "prettier": "^2.8.1",
    "prisma": "^4.8.1",
    "typescript": "^4.9.4"
  }
}
