{
  "name": "smolbot",
  "version": "2.0.0",
  "description": "Custom mod/misc/util bot",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "start:dev": "tsc --build && node -r dotenv/config dist/index.js",
    "start:prod": "npx prisma migrate deploy && node dist/index.js",
    "build": "tsc --build",
    "lint": "eslint src/ && prettier --check src/ && tsc --noEmit",
    "lint:fix": "eslint src/ --fix && prettier --write src/ && tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AtoraSuunva/smolbot.git"
  },
  "author": "AtoraSuunva",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AtoraSuunva/smolbot/issues"
  },
  "homepage": "https://github.com/AtoraSuunva/smolbot#readme",
  "dependencies": {
    "@prisma/client": "~4.14.0",
    "@sapphire/shapeshift": "^3.9.0",
    "@types/convert-units": "~2.3.5",
    "convert-units": "~2.3.4",
    "csv-parse": "^5.3.10",
    "csv-stringify": "^6.4.0",
    "discord.js": "~14.11.0",
    "dotenv": "~16.0.3",
    "emoji-regex": "~10.2.1",
    "env-var": "~7.3.1",
    "humanize-duration": "~3.28.0",
    "pino": "~8.14.1",
    "pino-pretty": "~10.0.0",
    "pretty-ms": "~8.0.0",
    "sleetcord": "~0.9.0",
    "undici": "~5.22.1"
  },
  "devDependencies": {
    "@types/humanize-duration": "~3.27.1",
    "@types/node": "~20.1.3",
    "@typescript-eslint/eslint-plugin": "~5.59.5",
    "@typescript-eslint/parser": "~5.59.5",
    "eslint": "~8.40.0",
    "eslint-config-prettier": "~8.8.0",
    "eslint-plugin-import": "~2.27.5",
    "prettier": "~2.8.8",
    "prisma": "~4.14.0",
    "typescript": "~5.0.4"
  }
}
