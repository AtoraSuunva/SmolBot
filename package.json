{
  "name": "smolbot",
  "version": "2.0.0",
  "description": "Custom mod/misc/util bot",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "main": "node --loader ts-node/esm -r dotenv/config src/index.ts | pino-pretty",
    "build": "tsc --build",
    "lint": "eslint src/ && prettier --check src/ && tsc --noEmit",
    "lint:fix": "eslint src/ --fix && prettier --write src/ && tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AtoraSuunva/smolbot.git"
  },
  "author": "AtoraSuunva",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AtoraSuunva/smolbot/issues"
  },
  "homepage": "https://github.com/AtoraSuunva/smolbot#readme",
  "dependencies": {
    "discord-api-types": "^0.36.2",
    "discord.js": "^13.8.1",
    "env-var": "^7.1.1",
    "eslint-plugin-import": "^2.26.0",
    "pino": "^8.1.0",
    "sleetcord": "^0.0.4",
    "undici": "^5.7.0"
  },
  "devDependencies": {
    "@swc/core": "^1.2.215",
    "@types/node": "^18.0.4",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "dotenv": "^16.0.1",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "pino-pretty": "^8.1.0",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
