{
  "name": "smolbot",
  "version": "2.0.0",
  "description": "Custom mod/misc/util bot",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "main": "node -r dotenv/config dist/index.js",
    "dev": "node --loader ts-node/esm -r dotenv/config src/index.ts | pino-pretty",
    "build": "tsc --build",
    "lint": "eslint src/ && prettier --check src/ && tsc --noEmit",
    "lint:fix": "eslint src/ --fix && prettier --write src/ && tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AtoraSuunva/smolbot.git"
  },
  "author": "AtoraSuunva",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AtoraSuunva/smolbot/issues"
  },
  "homepage": "https://github.com/AtoraSuunva/smolbot#readme",
  "dependencies": {
    "@prisma/client": "^4.2.0",
    "discord.js": "^14.1.2",
    "dotenv": "^16.0.1",
    "emoji-regex": "^10.1.0",
    "env-var": "^7.1.1",
    "pino": "^8.4.0",
    "sleetcord": "^0.3.1",
    "undici": "^5.8.2"
  },
  "devDependencies": {
    "@types/node": "^18.6.5",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "pino-pretty": "^8.1.0",
    "prettier": "^2.7.1",
    "prisma": "^4.2.0",
    "typescript": "^4.7.4"
  }
}
